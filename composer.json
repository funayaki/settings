{
    "name": "funayaki/settings",
    "description": "Croogo Settings Plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Croogo Development Team",
            "email": "team@croogo.org"
        }
    ],
    "type": "cakephp-plugin",
    "autoload": {
        "psr-4": {
            "Settings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Settings\\Test\\": "tests",
            "App\\": "tests/test_app/src/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "~3.5.0",
        "friendsofcake/search": "dev-master",
        "admad/cakephp-sequence": "dev-master"
    }
}
